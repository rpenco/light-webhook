{
  "name": "light-webhook",
  "version": "2.0.0-alpha.0",
  "description": "Simple webhook client. Receives requests from Github, Gitlab, or HTTP and executes HTTP requests or Bash commands.\n",
  "homepage": "https://github.com/rpenco/light-webhook#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "./dist/bundle.js",
  "bin": {
    "light-webhook": "./dist/bundle.js"
  },
  "scripts": {
    "test": "mocha -r ts-node/register ./\\*\\*/\\*.spec.ts",
    "build": "webpack",
    "start:dev": "node src/index.ts -c ./examples/http/http.yaml",
    "release:pack": "npm run build && npm pack",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpenco/light-webhook.git"
  },
  "keywords": [
    "webhook",
    "bash",
    "github",
    "gitlab",
    "client",
    "nodejs",
    "webhook-client",
    "hook",
    "light"
  ],
  "contributors": [],
  "author": {
    "name": "Romain Penco",
    "url": "https://github.com/rpenco"
  },
  "bugs": {
