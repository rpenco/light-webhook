{
  "github": {
    "subscribe": [
      {
        "service": "github",
        "name": "github",
        "settings":{
          "events": ["*"],
          "secret": false
        }
      }
    ],
    "publish": [
      {
        "service": "bash",
        "name": "ls-dir",
        "settings": {
          "cmd": [
            "echo",
            "'event: {{headers.x-github-event}}, url: {{body.hook.url}}'"
          ]
        }
      }
    ]
  }
}
